{
    "name": "eliseekn/tinymvc",
    "description": "PHP framework based on MVC architecture",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "app/Functions.php",
            "framework/Support/Helpers.php"
        ],
        "psr-4": {
            "App\\" : "app/",
            "Framework\\" : "framework/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": "^7.2",
        "wixel/gump": "^1.11",
        "phpmailer/phpmailer": "^6.1",
        "caseyamcl/configula": "^3.1",
        "dompdf/dompdf": "^0.8.6",
        "nesbot/carbon": "^2.41",
        "pragmarx/countries": "^0.7.2",
        "twig/twig": "^3.0",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "fzaninotto/faker": "^1.9"
    }
}
